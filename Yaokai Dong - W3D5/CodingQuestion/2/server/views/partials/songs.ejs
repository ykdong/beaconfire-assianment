<% if (success) { %>
  <table>
    <thead>
      <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Language</th>
        <th>Genre</th>
        <th>Artist</th>
      </tr>
    </thead>
    <tbody>
      <% songs.forEach(song => { %>
        <tr>
          <td><%= song.id %></td>
          <td><%= song.title %></td>
          <td><%= song.language %></td>
          <td><%= song.genre %></td>
          <td><%= song.artist %></td>
        </tr>
      <% }); %>
    </tbody>
  </table><hr>
<% } else { %>
  <h3>No saved songs yet</h3>
<% } %>
